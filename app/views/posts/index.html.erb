<div class="field">
    
    <%= f.text_field :firstname %>
  </div>

<h1>

    <th>Firstname</th>
    <th>Lastname</th>
    <th>Age</th>
    <th>Address</th>
    <th>Tel</th>
    <th>Q1</th>
