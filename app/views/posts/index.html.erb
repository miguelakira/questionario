<% @posts.each do |post| %>
  <tr>
    <td><%= post.firstname %></td>
    <td><%= post.lastname %></td>
    <td><%= post.age %></td>
    <td><%= post.address %></td>
    <td><%= post.tel %></td>
    <td><%= post.q1 %></td>
    <td><%= post.q2 %></td>
    <td><%= post.q3 %></td>
    <td><%= post.q4 %></td>
    <td><%= post.q5 %></td>
    <td><%= post.q6 %></td>
    <td><%= post.q7 %></td>
    <td><%= post.q8 %></td>
    <td><%= post.q9 %></td>
    <td><%= post.q10 %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, :confirm => 'Are you sure?', :method => :delete %></td>
<%end  %>
